{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "acacha/admin-lte-template-laravel": "4.*",
        "alaouy/youtube": "dev-master",
        "albertcht/invisible-recaptcha": "^1.4",
        "artesaos/seotools": "^0.10.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.3.",
        "ramsey/laravel-oauth2-instagram": "^2.0",
        "sammyk/laravel-facebook-sdk": "^3.0",
        "simplesoftwareio/simple-sms": "3.0.*",
        "spatie/laravel-analytics": "^3.0",
        "spatie/laravel-newsletter": "^3.5",
        "spatie/laravel-sitemap": "^2.2",
        "thujohn/twitter": "^2.2",
        "torann/geoip": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "cbrcleaning\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
